{
  "name": "app_name",
  "version": "0.0.1",
  "description": "front app skelton project",
  "main": "index.js",
  "scripts": {
    "html-build": "node ./node_modules/.bin/pug --pretty --watch src/pug/index.pug -o ./",
    "css-build": "node ./node_modules/.bin/stylus -w src/stylus/style.styl --out ./dist/css",
    "js-build": "node ./node_modules/.bin/webpack --progress --colors --watch",
    "build": "npm run html-build & npm run css-build & npm run js-build",
    "browser-sync": "node ./node_modules/.bin/browser-sync start --server --files=\"./index.html, ./dist/css/*.css, ./dist/js/*.js\"",
    "develop": "npm run build & npm run browser-sync"
  },
  "author": "Tomohiro_Kino",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "browser-sync": "^2.14.0",
    "pug": "^2.0.0-beta4",
    "pug-cli": "^1.0.0-alpha6",
    "stylus": "^0.54.5",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0"
  }
}
